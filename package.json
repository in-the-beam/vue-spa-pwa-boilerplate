{
  "name": "vue-spa-boilerplate",
  "version": "0.2.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode=development",
    "prod": "vue-cli-service build --mode=production",
    "watch": "vue-cli-service build --watch --mode=development",
    "css-watch": "node-sass src/sass/main.scss -o dist/css -r --output-style expanded --watch --indent-type space --indent-width 2 --importer node_modules/node-sass-import",
    "css-dev": "node-sass src/sass/main.scss -o dist/css -r --output-style expanded --indent-type space --indent-width 2 --importer node_modules/node-sass-import",
    "css-prod": "node-sass src/sass/main.scss -o dist/css/min -r --output-style compressed --indent-type space --indent-width 0 --importer node_modules/node-sass-import"
  },
  "author": "Stanislav Kabin <me@h-zone.ru>",
  "license": "MIT",
  "devDependencies": {
    "@babel/compat-data": "^7.13.12",
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.12",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^5.0.0-alpha.8",
    "@vue/cli-service": "^4.5.12",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "extract-loader": "^5.1.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass-import": "^1.1.1",
    "path": "^0.12.7",
    "postcss-loader": "^4.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0"
  },
