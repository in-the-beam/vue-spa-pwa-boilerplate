  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode=development",
    "prod": "vue-cli-service build --mode=production",
    "watch": "vue-cli-service build --watch --mode=development",
    "css-watch": "node-sass src/sass/main.scss -o dist/css -r --output-style expanded --watch --indent-type space --indent-width 2 --importer node_modules/node-sass-import",
